{
  "name": "rs-vna-test-automation",
  "productName": "R&S VNA Test Automation for Desktop",
  "version": "0.1.0",
  "description": "Simple ATE for Rohde & Schwarz VNAs.",
  "author": "Nick Lalic",
  "homepage": "https://vna.rs-us.net/applications/test_automation.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/Terrabits/rs-vna-test-automation.git"
  },
  "private": true,
  "main": "main.js",
  "dependencies": {
    "wait-on": "^3.3.0"
  },
  "scripts": {
    "start": "concurrently \"./server/test-automation --http-only\" \"electron .\"",
    "dist": "build"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^5.0.4",
    "electron-builder": "^20.43.0",
    "serve": "^11.0.2"
  },
  "build": {
    "asar": false,
    "appId": "com.rohde-schwarz.test-automation-desktop",
    "productName": "R&S Test Automation for Desktop",
    "files": [
      "src/**/*",
      "server/test-automation",
      "LICENSE.txt",
      "main.js"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "perMachine": true,
      "menuCategory": "Rohde-Schwarz"
    },
    "extends": null
  }
}
